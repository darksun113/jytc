{
	"easycom": {
		"^u-(.*)": "uview-ui/components/u-$1/u-$1.vue"
	},
	"pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		{
			"path": "pages/home/home",
			"style": {
				"navigationBarTitleText": "首页",
				"navigationBarTextStyle": "white"
			}
		}, {
			"path": "pages/user/user",
			"style": {
				"navigationBarTitleText": "我的",
				"enablePullDownRefresh": false
			}

		}
	],
	"tabBar": {
		"color": "#6667AB", //字体颜色
		"selectedColor": "#FFFFFF", //选中时字体颜色
		"backgroundColor": "#090c47", //底部背景色
		"fontSize": "20rpx", //字体大小
		"spacing": "10rpx", //字体到图标的距离
		"height": "156rpx", //底部高
		"list": [{
				"pagePath": "pages/home/home",
				"iconPath": "static/tabBar/home_tab_icon.svg", //未选中时图片样式
				"selectedIconPath": "static/tabBar/home_tab_s_icon.svg", //选中时图片样式
				"text": "首页"
			},
			{
				"pagePath": "pages/user/user",
				"iconPath": "static/tabBar/user_tab_icon.svg",
				"selectedIconPath": "static/tabBar/user_tab_s_icon.svg",
				"text": "我的"
			}
		]
	},
	"subPackages": [{
		"root": "subpageA",
		"pages": [{
			"path": "LoginPage/LoginPage",
			"style": {
				"navigationBarTitleText": "登录",
				"enablePullDownRefresh": false,
				"navigationBarTextStyle": "white",
				"navigationBarBackgroundColor": "#04030A"
			}
		}, {
			"path": "Certification/Certification",
			"style": {
				"navigationBarTitleText": "",
				"enablePullDownRefresh": false
			}

		}, {
			"path": "SeriesPage/SeriesPage",
			"style": {
				"navigationBarTitleText": "",
				"enablePullDownRefresh": false
			}

		}, {
			"path": "DetailPage/DetailPage",
			"style": {
				"navigationBarTitleText": "详情",
				"enablePullDownRefresh": false
			}

		}    ,{
                    "path" : "SharePage/SharePage",
                    "style" :                                                                                    
                {
                    "navigationBarTitleText": "",
                    "enablePullDownRefresh": false
                }
                
                }
            ]
	}],
	"globalStyle": {
		"navigationBarTextStyle": "white",
		"navigationBarTitleText": "金裕天成",
		"navigationBarBackgroundColor": "#04030A",
		"backgroundColor": "#04030A"
	}
}
